{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Launch AppleFitnessWorkoutMapper",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "build",
          "program": "${workspaceFolder}/src/AppleFitnessWorkoutMapper/bin/Debug/net9.0/AppleFitnessWorkoutMapper.dll",
          "args": [],
          "cwd": "${workspaceFolder}/src/AppleFitnessWorkoutMapper",
          "stopAtEntry": false,
          "serverReadyAction": {
              "action": "openExternally",
              "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
          },
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development"
          },
          "sourceFileMap": {
              "/Views": "${workspaceFolder}/Views"
          }
      }
  ]
}
