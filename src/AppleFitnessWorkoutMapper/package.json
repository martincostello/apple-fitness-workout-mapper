{
  "name": "applefitnessworkoutmapper",
  "private": true,
  "version": "1.2.0",
  "description": "Visualise multiple journeys from Apple Fitness on a map",
  "scripts": {
    "build": "npm run compile && npm run format && npm run lint && npm test",
    "compile": "webpack",
    "format": "prettier --write scripts/**/*.ts",
    "format-check": "prettier --check scripts/**/*.ts",
    "lint": "eslint --ext .ts scripts",
    "test": "jest --coverage",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martincostello/apple-fitness-workout-mapper.git"
  },
  "author": "martincostello",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@types/google.maps": "^3.52.5",
    "@types/jquery": "^3.5.16",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^28.1.3",
    "moment": "^2.29.4",
    "prettier": "^2.8.7",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.4.2",
    "tsify": "^5.0.4",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.1"
  }
}
